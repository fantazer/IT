@import 'rupture'
@import "variable/_function.styl"
@import "variable/_fonts.styl"
@import "variable/_ie.styl"

@import "grid/grid.css"
@import "grid/extends.css"

@import "variable/config/_config.styl"

@import "variable/config/_typography.styl"
@import "variable/config/_btn.styl"
@import "variable/config/_color.styl"
@import "variable/config/_input.styl"

@import "variable/_helper.styl"
@import "variable/_default.styl"


body
	@extend .type--main
	font-family $font
	//color $typeMain
	color black
	line-height 1.4
	//letter-spacing 0.01em

.main-cont-sm
	center(800px,20px)

.main-cont-md
	center(1000px,20px)

.main-cont
	center(1200px,20px)

.main-cont-lg
	center(1600px,20px)

.scroll-template
	+above(1025px)
		&::-webkit-scrollbar
			-webkit-appearance: none
			width: 4px
			height: 8px
			background: #DCDFE8
			border-radius: 4px
		&::-webkit-scrollbar-thumb
			border-radius: 4px
			background: $mark
		scrollbar-color  #DCDFE8 #f8f8f8
		scrollbar-width thin
		overscroll-behavior contain
.scroll-template-light
	+above(1025px)
		&::-webkit-scrollbar
			-webkit-appearance: none
			width: 4px
			height: 8px
			background: #f8f8f8
			border-radius: 4px
		&::-webkit-scrollbar-thumb
			border-radius: 4px
			background: $border-color
		scrollbar-color #DCDFE8 #f8f8f8
		scrollbar-width thin
		overscroll-behavior contain


.no-mark
	transition none
	-webkit-touch-callout: none;
	-webkit-user-select: none;
	-khtml-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	user-select: none;
	-webkit-tap-highlight-color: transparent;
//.header
//.main-cont
.header-top
	+below(1024px)
		padding: 0
		height: 50px
	flex(between)
.header-top__logo
	@extend .type--xl
	color black
	font-family:'PT Sans'

//font-family:Roboto
.header-top__search
	+below(1024px)
		display: none
	width: 45%
	input
		border-radius 24px
		background none
		border 1px solid #ffffff0f
		//background: #172e9e
		padding: 10px 20px
		&::-webkit-input-placeholder
			color:#ffffff70
		&::-moz-placeholder
			color:#ffffff70
		&:-moz-placeholder
			color:#ffffff70
		&:-ms-input-placeholder
			color:#ffffff70
		color white
		&:hover
			border 1px solid #ffffff63
		width: 100%
.header-top__nav
	+below(1024px)
		height: 100%
	f()
	align-items: center
.header-top__nav-lk
	+below(1024px)
		height: 100%
		flex(mid)
	+below(640px)
		display: none
	&:hover
		.icon
			fill $mark-bright
	cp()
	padding: 10px 20px
	border-left 1px solid $border-color
	border-right 1px solid $border-color
	.icon
		+below(1024px)
			size 20px
		tr(all)
		size 30px
		fill $mark
//.icon
.header-top__nav-cource
	+below(640px)
		display: none
	&:hover
		.icon
			fill $mark-bright
	cp()
	padding: 10px 20px
	border-left 1px solid $border-color
	border-right 1px solid $border-color
	margin-left: -1px
	.icon
		+below(1024px)
			size 20px
		tr(all)
		size 30px
		fill $mark
.header-top__nav-phone
	@extend .type--md
	font-weight:700
	color black
	margin-left: 20px
.header-nav-wrap
	+below(1024px)
		width: 320px
		position: fixed
		right 0
		height: calc(100% - 50px)
		z-index 1250
		top 50px
		transform translateX(100%)
		tr(all)
		.main-cont
			padding: 0
	+below(640px)
		width: 100%
	//background: #000
	//background: $mark
	background: #3e51ab
	&.header-nav-wrap--open
		transform translateX(0%)
		overflow-y: auto

.header-nav
	+below(1024px)
		height: 100vh
		fl()
	color white
	f()
.header-nav__el
	+below(1024px)
		width: 100%
		border 0
		align-items: flex-start
		border-bottom 1px solid #ffffff0f
		fl()
		text-align: left
		font-size: 16px
		.icon
			display: none
	flex(mid)
	tr(all)
	border-left 1px solid #ffffff0f
	border-right 1px solid #ffffff0f
	margin-left: -1px
	position: relative
	cp()
	&:hover
		background: $mark-bright
	.icon
		size 20px
		fill white
	&:hover
		+above(1025px)
			.header-nav-sub
				display: block
			.content-filter
				opacity 1
				left 0
.header-nav__el-text
	f()
	align-items: center
	color white!important
	font-family:Museo
	padding: 15px 30px
	display: block
	font-size: 14px
.header-nav__el-wrap
	display: block
.icon-nav-menu
	margin-right: 20px
.header-nav-sub
	display: none
	position: absolute
	top 100%
	left 0
	background: white
	width: 300px
	z-index 3
.header-nav-sub__el
	padding: 15px
	color black
	border-bottom 1px solid $border-color
	flex(between)
	cp()
	font-family:Museo
	font-weight:700
	font-size: 13px
	&:hover
		& >.header-nav-sub-two
			display: block
	.icon
		size 10px
		fill #818181
.content
	position: relative
	flex-grow 1

	//background: #EDEDED
.content-filter
	position: absolute
	width: 100%
	height: 100%
	background: rgba(0, 0, 0, 0.3)
	opacity:0
	left -9999px
	z-index 2

.content-filter--show
	display: none

.content-filter--true
	opacity:1
	left 0

.header-nav-sub-wrap
	position: relative
.header-nav-sub-two
	position: absolute
	left: 100%
	top 0
	height: 100%
	width: 300px
	display: none
	background: white
	z-index 3

.header-nav-sub__el-two
	padding: 15px
	color black
	border-bottom 1px solid $border-color
	cp()
	//text-overflow: ellipsis
	//overflow: hidden
	//white-space: nowrap
	span
		font-weight:700

	.icon
		display: none
		size 10px
		fill #818181

.banner
	flex(between)
	align-items: stretch
.banner__el
